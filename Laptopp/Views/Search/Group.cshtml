@model IEnumerable<IGrouping<int?, Laptopp.Models.LAP>>
@{
    ViewBag.Title = "Group";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}



<h2>DANH SÁCH THEO Thương Hiệu </h2>
@foreach (var item in Model)
{
    <div>
        <h4 style="font-weight:bold; color:red;">MÃ THƯƠNG HIỆU @item.Key</h4>
        <table border="1">
            <tr style="background-color:burlywood; font-weight:bold;">
                <td>Mã laptop</td>
                <td>Tên laptop</td>
                <td>Tên TH</td>
            </tr>
            @foreach (var lap in item)
            {
                <tr>
                    <td>@lap.MaLap</td>
                    <td>@lap.TenLap</td>
                    <td>@lap.THUONGHIEU.TenThuongHieu</td>
                </tr>
            }
        </table>
    </div>
}
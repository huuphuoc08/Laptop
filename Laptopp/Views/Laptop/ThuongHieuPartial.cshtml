@model IEnumerable<Laptopp.Models.THUONGHIEU>
<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">
        Laptop theo thương hiệu
    </li>
    @foreach (var item in Model)
    {
        <a href="@Url.Action("LapTheoThuongHieu","Laptop", new { iMaTH = item.MaTH})"
           class="list-group-item">@item.TenThuongHieu</a>
    }
</ul>
